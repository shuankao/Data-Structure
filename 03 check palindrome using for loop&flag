#include<iostream>
#include<string.h>
#include<sstream>

using namespace std;
int main()
{
	string str;
	getline(cin, str);
	int n = str.length();
	int flag = 1;

		for (int i = 0; i < n / 2; i++)
		{
			if (str[i] == str[n-1-i])
			{
				continue;
			}
			else
			{
				flag = 0;
				break;
			}
		}

		if (flag ==1)
		{
			cout << "yes" << endl;
		}
		else
		{
			cout << "no" << endl;
		}

}
