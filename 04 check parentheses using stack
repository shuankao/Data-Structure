#include<iostream>
#include<string.h>
#include<sstream>
#include<stack>

using namespace std;
int main()
{
	stack<char> s;
	string a;
	while(cin >> a)
	{
		int flag = 0;
		int n = a.length();
		for (int i = 0; i < n; i++)
		{
			if (a.at(i) == '(')
			{
				s.push('(');
				
			}
			else
			{
				if (!s.empty())
				{
					s.pop();
				}
				else
				{
					cout<< "error";
					flag = 1;
					break;
				}
			}
		}
		if (flag == 0)
		{
			if (s.empty())
			{
				cout << "correct";
			}
			else
			{
				cout << "error";
			}
		}
	}
	return 0;
}
