#include<iostream>

using namespace std;

int times = 0;

void Hanoi(int n, char A, char B, char C)
{

	if (n == 1)
	{
		cout << "move from " << A << " to " << C <<endl;
		times++;
	}
	else
	{
		Hanoi(n - 1, A, C, B);
		Hanoi(1, A, B, C);
		Hanoi(n - 1, B, A, C);
	}
}
int main()
{
	int n;
	cin >> n;
	Hanoi(n, 'A', 'B', 'C');
	cout << "move " << times << " times in total" << endl;
	return 0;
}
